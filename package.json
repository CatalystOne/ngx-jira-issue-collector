{
  "name": "ngx-jira-issue-collector",
  "version": "0.0.0-semantically-released",
  "description": "Jira Issue Collector as an angular component",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ngx-jira-issue-collector --configuration production",
    "test": "ng test --browsers=ChromeHeadlessCI --watch=false",
    "lint": "npm run lint:check",
    "lint:check": "prettier --check **/*.{js,json,scss,md,ts,html}",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint:format": "prettier --write **/*.{js,json,scss,md,ts,html}",
    "sonar": "sonar-scanner",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CatalystOne/ngx-jira-issue-collector.git"
  },
  "bugs": {
    "url": "https://github.com/CatalystOne/ngx-jira-issue-collector/issues"
  },
  "homepage": "https://github.com/CatalystOne/ngx-jira-issue-collector",
  "author": {
    "name": "Kasper Bjerke"
  },
  "license": "MIT",
  "private": false,
  "peerDependencies": {
    "@angular/common": "^14.0.0 || ^15.0.0",
    "@angular/core": "^14.0.0 || ^15.0.0",
    "@angular/platform-browser": "^14.0.0 || ^15.0.0",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.11",
    "@angular/cli": "14.2.11",
    "@angular/common": "15.2.3",
    "@angular/compiler": "15.2.3",
    "@angular/compiler-cli": "15.2.3",
    "@angular/core": "15.2.3",
    "@angular/language-service": "15.2.3",
    "@angular/platform-browser": "15.2.3",
    "@angular/platform-browser-dynamic": "15.2.3",
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/git": "10.0.1",
    "@types/jasmine": "4.3.1",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "18.15.3",
    "codelyzer": "6.0.2",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "jasmine-core": "4.5.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "lint-staged": "13.2.0",
    "ng-packagr": "14.2.2",
    "prettier": "2.8.4",
    "protractor": "7.0.0",
    "rxjs": "7.5.7",
    "semantic-release": "19.0.5",
    "semantic-release-ado": "1.4.0",
    "sonarqube-scanner": "2.9.1",
    "ts-node": "10.9.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.8.4",
    "zone.js": "0.12.0"
  },
  "dependencies": {
    "tslib": "2.4.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{js,json,scss,md,ts,html}": [
      "prettier --write"
    ]
  }
}
