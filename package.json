{
  "name": "ngx-jira-issue-collector",
  "version": "0.0.0-semantically-released",
  "description": "Jira Issue Collector as an angular component",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "test": "ng test --browsers=ChromeHeadlessCI --watch=false",
    "lint": "npm run lint:check",
    "lint:check": "prettier --check **/*.{js,json,scss,md,ts,html}",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint:format": "prettier --write **/*.{js,json,scss,md,ts,html}",
    "sonar": "sonar-scanner",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CatalystOne/ngx-jira-issue-collector.git"
  },
  "bugs": {
    "url": "https://github.com/CatalystOne/ngx-jira-issue-collector/issues"
  },
  "homepage": "https://github.com/CatalystOne/ngx-jira-issue-collector",
  "author": {
    "name": "Kasper Bjerke"
  },
  "license": "MIT",
  "private": false,
  "peerDependencies": {
    "@angular/common": "^12.2.6",
    "@angular/core": "^12.2.6",
    "@angular/platform-browser": "^12.2.6",
    "rxjs": "~7.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.15",
    "@angular/cli": "12.2.15",
    "@angular/common": "12.2.15",
    "@angular/compiler": "12.2.15",
    "@angular/compiler-cli": "12.2.15",
    "@angular/core": "12.2.15",
    "@angular/language-service": "12.2.15",
    "@angular/platform-browser": "12.2.15",
    "@angular/platform-browser-dynamic": "12.2.15",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.1",
    "@types/jasmine": "3.10.3",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "16.11.21",
    "codelyzer": "6.0.2",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.4",
    "jasmine-core": "3.99.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.14",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "lint-staged": "11.2.6",
    "ng-packagr": "12.2.6",
    "prettier": "2.4.1",
    "protractor": "7.0.0",
    "rxjs": "7.3.0",
    "semantic-release": "17.4.7",
    "semantic-release-ado": "1.2.2",
    "sonar-scanner": "3.1.0",
    "ts-node": "10.4.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.3.5",
    "zone.js": "0.11.4"
  },
  "dependencies": {
    "tslib": "2.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{js,json,scss,md,ts,html}": [
      "prettier --write"
    ]
  }
}
